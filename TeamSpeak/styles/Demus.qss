}

QTabBar {		/* NOTE: the 1px "border" at the top is a 3D light effect which cannot be changed with qss right now, bc QPalette::Light is not accessable */
	background: transparent;
}

QTabBar::tab {
	padding-left: 10px;
	padding-right: 10px;
	background-color: #1F1F1F;
	color: #EEEEEE;
	min-height: 32px;
	border: none;
}

QTabBar::tab:hover {
	background: #9F1D35;
}

