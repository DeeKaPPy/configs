	background: #005677;
}

QTabBar::tab:disabled {
	color: #555555;
}

QTabBar::tear {
    image: none;
}

QTabBar::scroller {
    width: 27px;
}

QTabBar QToolButton {
	background-color: #1F1F1F;
	margin: 0px;
	padding: 6px;
